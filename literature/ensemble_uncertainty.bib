@article{ ISI:000233460000005,
Author = {Hamill, TM and Whitaker, JS},
Title = {{Accounting for the error due to unresolved scales in ensemble data
   assimilation: A comparison of different approaches}},
Journal = {{MONTHLY WEATHER REVIEW}},
