@article{ ISI:000184611500001,
Author = {Schmid, HP and Su, HB and Vogel, CS and Curtis, PS},
Title = {{Ecosystem-atmosphere exchange of carbon dioxide over a mixed hardwood
   forest in northern lower Michigan}},
