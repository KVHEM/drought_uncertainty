@article{ ISI:000180827200002,
Author = {Khairoutdinov, MF and Randall, DA},
Title = {{Cloud resolving modeling of the ARM summer 1997 IOP: Model formulation,
   results, uncertainties, and sensitivities}},
Journal = {{JOURNAL OF THE ATMOSPHERIC SCIENCES}},
