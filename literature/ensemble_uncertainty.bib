@article{ ISI:000232759300010,
Author = {Pappenberger, F and Beven, KJ and Hunter, NM and Bates, PD and
   Gouweleeuw, BT and Thielen, J and de Roo, APJ},
Title = {{Cascading model uncertainty from medium range weather forecasts (10
   days) through a rainfall-runoff model to flood inundation predictions
   within the European Flood Forecasting System (EFFS)}},
Journal = {{HYDROLOGY AND EARTH SYSTEM SCIENCES}},
Year = {{2005}},
