@article{ ISI:000264085900009,
Author = {Della-Marta, Paul M. and Mathis, Hubert and Frei, Christoph and Liniger,
   Mark A. and Kleinn, Jan and Appenzeller, Christof},
Title = {{The return period of wind storms over Europe}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},
