@article{ ISI:000331489100005,
Author = {Ershadi, A. and McCabe, M. F. and Evans, J. P. and Chaney, N. W. and
   Wood, E. F.},
Title = {{Multi-site evaluation of terrestrial evaporation models using FLUXNET
   data}},
Journal = {{AGRICULTURAL AND FOREST METEOROLOGY}},
