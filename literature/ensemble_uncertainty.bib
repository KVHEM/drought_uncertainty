@article{ ISI:000258190100013,
Author = {Zhou, Tianjun and Yu, Rucong and Li, Hongmei and Wang, Bin},
Title = {{Ocean forcing to changes in global monsoon precipitation over the recent
   half-century}},
Journal = {{JOURNAL OF CLIMATE}},
