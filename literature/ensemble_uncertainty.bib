@article{ ISI:000285426000006,
Author = {Christensen, Jens Hesselbjerg and Kjellstrom, Erik and Giorgi, Filippo
   and Lenderink, Geert and Rummukainen, Markku},
Title = {{Weight assignment in regional climate models}},
