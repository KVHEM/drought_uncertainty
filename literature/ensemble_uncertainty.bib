@article{ ISI:000238689800016,
Author = {Pan, M and Wood, EF},
Title = {{Data assimilation for estimating the terrestrial water budget using a
   constrained ensemble Kalman filter}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
Year = {{2006}},
