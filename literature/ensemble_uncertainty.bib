@article{ ISI:000325272000007,
Author = {Jones, Gareth S. and Stott, Peter A. and Christidis, Nikolaos},
Title = {{Attribution of observed historical near-surface temperature variations
   to anthropogenic and natural causes using CMIP5 simulations}},
