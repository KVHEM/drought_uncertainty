Title = {{Evaluation of changes in deficit volumes: support for protection of
   localities suitable for construction of reservoirs}},
Booktitle = {{CONSIDERING HYDROLOGICAL CHANGE IN RESERVOIR PLANNING AND MANAGEMENT}},
Series = {{IAHS Publication}},
Year = {{2013}},
