Author = {Goddard, L. and Aitchellouche, Y. and Baethgen, W. and Dettinger, M. and
   Graham, R. and Hayman, P. and Kadi, M. and Martinez, R. and Meinke, H.
   and Conrad, E.},
Editor = {{Sivakumar, MVK and Nyenzi, BS and Tyagi, A}},
Title = {{Providing Seasonal-to-Interannual Climate Information for Risk
   Management and Decision-making}},
