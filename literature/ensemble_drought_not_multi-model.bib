@article{ ISI:000339697000016,
Author = {Shah, Reepal and Mishra, Vimal},
Title = {{Evaluation of the Reanalysis Products for the Monsoon Season Droughts in
   India}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
