@article{ ISI:000290449200009,
Author = {Tsanis, Ioannis K. and Koutroulis, Aristeidis G. and Daliakopoulos,
   Ioannis N. and Jacob, Daniela},
Title = {{Severe climate-induced water shortage and extremes in Crete}},
Journal = {{CLIMATIC CHANGE}},
