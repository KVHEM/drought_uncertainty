@inproceedings{ ISI:000357968700082,
Author = {Zappa, Massimiliano and Bernhard, Luzi and Spirig, Christoph and
   Pfaundler, Martin and Stahl, Kerstin and Kruse, Sylvia and Seidl, Irmi
   and Staehli, Manfred},
Editor = {{Castellarin, A and Ceola, S and Toth, E and Montanari, A}},
Title = {{A prototype platform for water resources monitoring and early
   recognition of critical droughts in Switzerland}},
Booktitle = {{EVOLVING WATER RESOURCES SYSTEMS: UNDERSTANDING, PREDICTING AND MANAGING
   WATER-SOCIETY INTERACTIONS}},
