@article{ ISI:000221831700015,
Author = {Lin, ZH and Wang, HJ and Zhou, GQ and Chen, H and Lang, XM and Zhao, Y
   and Zeng, QC},
Title = {{Recent advances in dynamical extra-seasonal to annual climate prediction
   at IAP/CAS}},
