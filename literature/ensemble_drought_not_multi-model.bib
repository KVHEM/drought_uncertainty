@article{ ISI:000236606600002,
Author = {Herweijer, C and Seager, R and Cook, ER},
Title = {{North American droughts of the mid to late nineteenth century: a
   history, simulation and implication for Mediaeval drought}},
Journal = {{HOLOCENE}},
Year = {{2006}},
Volume = {{16}},
Number = {{2}},
