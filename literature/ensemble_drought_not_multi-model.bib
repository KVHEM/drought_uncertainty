@article{ ISI:000378741900008,
Author = {Forzieri, Giovanni and Feyen, Luc and Russo, Simone and Vousdoukas,
   Michalis and Alfieri, Lorenzo and Outten, Stephen and Migliavacca, Mirco
   and Bianchi, Alessandra and Rojas, Rodrigo and Cid, Alba},
Title = {{Multi-hazard assessment in Europe under climate change}},
