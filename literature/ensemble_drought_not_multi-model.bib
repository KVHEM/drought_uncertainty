@article{ ISI:000294058800006,
Author = {Brown, Iain and Poggio, Laura and Gimona, Alessandro and Castellazzi,
   Marie},
Title = {{Climate change, drought risk and land capability for agriculture:
   implications for land use in Scotland}},
Journal = {{REGIONAL ENVIRONMENTAL CHANGE}},
