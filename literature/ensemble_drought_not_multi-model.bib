@article{ ISI:000317926400008,
Author = {PaiMazumder, Debasish and Sushama, Laxmi and Laprise, Rene and Khaliq,
   M. Naveed and Sauchyn, Dave},
Title = {{Canadian RCM projected changes to short- and long-term drought
   characteristics over the Canadian Prairies}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},
